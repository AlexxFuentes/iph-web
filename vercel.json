{
    "version": 2,
    "public": true,
    "builds": [
      {
        "src": "index.html",
        "use": "@vercel/static"
      },
      {
        "src": "page/*.html",
        "use": "@vercel/static"
      },
      {
        "src": "style/*.css",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/page/(.*)",
        "dest": "/page/$1"
      }
    ]
  }
  